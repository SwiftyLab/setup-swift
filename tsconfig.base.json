{
    "compilerOptions": {
        "target": "ESNext",
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noImplicitReturns": true,
        "strictBuiltinIteratorReturn": false,
        "lib": [
            "ES2021.String",
        ]
    },
    "exclude": [
        "node_modules"
    ]
}
